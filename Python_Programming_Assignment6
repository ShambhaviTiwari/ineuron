#QUESTION 1
SELECT * FROM  TEST.CITY WHERE COUNTRYCODE = 'USA'  AND POPULATION> 100000;
#QUESTION 2
SELECT NAME FROM TEST.CITY WHERE COUNTRYCODE = 'USA' AND POPULATION > 120000;
#QUESTION 3
SELECT * FROM TEST.CITY;
#QUESTION 4
SELECT * FROM CITY WHERE ID = 1661;
#QUESTION 5
SELECT * FROM TEST.CITY WHERE COUNTRYCODE = 'JPN';
#QUESTION 6
SELECT NAME FROM TEST.CITY WHERE COUNTRYCODE = 'JPN';
#QUESTION 7
select city,state from station;
#QUESTION 8
SELECT DISTINCT(CITY) FROM STATION WHERE ID%2=0;
#QUESTION 9
SELECT COUNT(CITY)- COUNT(DISTINCT(CITY)) FROM STATION;
#QUESTION 10
SELECT CITY,LENGTH(CITY) FROM STATION GROUP BY CITY 
ORDER BY LENGTH(CITY) ASC, CITY ASC
LIMIT 1;

SELECT CITY,LENGTH(CITY) FROM STATION GROUP BY CITY 
ORDER BY LENGTH(CITY) DESC,CITY ASC
LIMIT 1;
#QUESTION 11
SELECT DISTINCT(CITY) FROM STATION WHERE CITY LIKE 'A%' OR CITY LIKE'E%' OR CITY LIKE'I%' OR CITY LIKE'O%' OR CITY LIKE'U%';
#QUESTION 12
SELECT DISTINCT(CITY) FROM STATION WHERE CITY LIKE '%A' OR CITY LIKE'%E' OR CITY LIKE'%I' OR CITY LIKE'%O' OR CITY LIKE'%U';

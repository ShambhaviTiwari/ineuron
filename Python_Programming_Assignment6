#QUESTION 1
SELECT * FROM  TEST.CITY WHERE COUNTRYCODE = 'USA'  AND POPULATION> 100000;
#QUESTION 2
SELECT NAME FROM TEST.CITY WHERE COUNTRYCODE = 'USA' AND POPULATION > 120000;
